record(ai, "$(P)$(R)DMarkY1-Mon") {
  field(DESC, "Get Y value of the marker")
  field(SCAN, "1 second")
  field(DISV, "0")
  field(SDIS, "$(P)$(R)EnblDMark1-Sts.RVAL")
  field(DTYP, "stream")
  field(PREC, "3")
  field(MDEL, "-1")
  field(ADEL, "-1")
  field(EGU, "dBm")
  field(INP, "@rsfsl.proto monDMarkY1 $(PORT)")
}

