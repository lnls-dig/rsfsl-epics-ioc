##### Maximum peak search for marker 2 
  
record(calcout, "$(P)$(R)EnblMaxAuto2") {
  field(SCAN, ".1 second")
  field(INPA, "0")
  field(CALC, "A=1?1:0")
  field(OUT, "$(P)$(R)OnOffMark2-Sel.RVAL")
  field(FLNK, "$(P)$(R)OnOffMark2-Sel")
}

record(bo, "$(P)$(R)OnOffMark2-Sel") {
  field(DESC, "Place Mark2 on Max Peak Automatically")
  field(DTYP, "stream")
  field(ZNAM, "OFF")
  field(ONAM, "ON")
  field(OUT, "@rsfsl.proto outOnOffM2($1) $(PORT)")
  field(FLNK, "$(P)$(R)DynamicX2-RB")
}

record(ai, "$(P)$(R)DynamicX2-RB") {
  field(DESC, "Get X value of the marker2")
  field(DISV, "0")
  field(SDIS, "$(P)$(R)EnblMaxAuto2.VAL")
  field(DTYP, "stream")
  field(PREC, "3")
  field(MDEL, "-1")
  field(ADEL, "-1")
  field(EGU, "Hz")
  field(INP, "@rsfsl.proto inMarkX2 $(PORT)")
}
 
